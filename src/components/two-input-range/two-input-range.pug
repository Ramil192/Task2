mixin two-input-range()
  section.two-input-range
    .two-input-range__arrival
        h3.two-input-range__header прибытие
        .two-input-range__data-drop-down
            label(for="start") 
            input.two-input-range__input.datepicker-here.datepickers(id="start" type="text" placeholder="ДД.ММ.ГГГГ" data-clear-Button="true" data-apply-Button="true")
    .two-input-range__departure
        h3.two-input-range__header выезд
        .two-input-range__data-drop-down
            label(for="end") 
            input.two-input-range__input.datepicker-here.datepickers(id="end" type="text" placeholder="ДД.ММ.ГГГГ" data-clear-Button="true" data-apply-Button="true")
                    